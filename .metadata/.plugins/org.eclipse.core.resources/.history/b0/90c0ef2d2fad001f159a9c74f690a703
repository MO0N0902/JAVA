package doWhileTest;
//1. do~while
public class DoWhileTest {
public static void main(String[] args) {
	
	boolean isFalse = false;
	
//	while(isFalse) {
//		System.out.println("안녕");
//		
//	}
//	System.out.println("출력끝 ");
//
	
	do {
		System.out.println("안녕");
	}while(isFalse) {
		
	}
	System.out.println("출력 끝");
	
}
	
	
}
