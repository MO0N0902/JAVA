package oper;

public class OperTest02 {
	public static void main(String[] args) {
		
		int num = 1;
		System.out.println(num);
		System.out.println(num + 10);
		
		//num = num + 10;
		System.out.println(num);
		num += 10;
		System.out.println(num);
		
		num*= 2;
		System.out.println(num);
		
		num/= 2;
		System.out.println(num);
	}
}
