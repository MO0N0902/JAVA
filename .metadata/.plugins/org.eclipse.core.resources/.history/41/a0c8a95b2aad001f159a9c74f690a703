package TwoForTest;
//3번 :  2중 for문 별찍기
public class ToForTest02 {
	public static void main(String[] args) {
		// *
		// ** 출력하기
	      for(int i = 1; i <= 3; i++) {
	         System.out.println("*"); //행
	         for(int j = 1; j <= 3; j++) { //열
	            System.out.print("*");
	         }
	         System.out.println();
	      }
		
	}
}
